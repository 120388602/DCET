cd %~dp0
set Protocol=..\Unity\Assets\Hotfix\Protocol\
set AutoGen=%Protocol%AutoGeneratedCode\
set LuaProtocol=..\Unity\Assets\Res\Lua\pbc\AutoGeneratedCode\

set cur=%cd%

for %%i in (*.proto) do (
    %cur%\protobuf-net\protogen -i:%%i -o:%AutoGen%%%~ni.cs
 )

for %%i in (*.proto) do (
    %cur%\protobuf-net\protoc %%i -o %cur%\%LuaProtocol%%%~ni.pb
 )

 cd %cur%
 echo protogen success